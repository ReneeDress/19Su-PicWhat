/* pages/index.wxss */
.grid {
  width: 120px;
	height: 120px;
	box-shadow: 0px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
	border-radius: 20px;
  border: solid 3px #ffffff;
  font-family: PingFang SC;
	font-size: 32px;
	font-weight: 500;
	font-stretch: normal;
	line-height: 45px;
	letter-spacing: 0px;
  color: #ffffff;
  text-align: right;
  margin: 15px;
  padding-top: 0px;
  padding-left: 0px;
  padding-right: 0px;
  text-indent: 18px;
  background-blend-mode: screen;
}
.crop {
  background-image: linear-gradient(210deg, 
  #ffdde1 0%, 
  #ee9ca7 100%), url("data:image/png;base64,");
}
.scan {
  background-image: linear-gradient(210deg, 
		#f2fcfe 0%, 
		#1c92d2 100%), url("data:image/png;base64,");
}
.beauty {
  background-image: linear-gradient(210deg, 
		#eaafc8 0%, 
		#654ea3 100%), url("data:image/png;base64,");
}
.filter {
  background-image: linear-gradient(210deg, 
		#99f2c8 0%, 
		#2193b0 100%), url("data:image/png;base64,");
}
.stitch {
  background-image: linear-gradient(210deg, 
		#ffd200 0%, 
		#f7971e 100%), url("data:image/png;base64,");
}
.about, .identity {
  background-image: linear-gradient(210deg, 
		#4286f4 0%, 
		#373b44 100%), url("data:image/png;base64,");
}
.circle {
  width: 420px;
	height: 420px;
	box-shadow: 0px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
	border-radius: 50%;
  border: solid 3px #ffffff;
}
.pink {
  background-image: radial-gradient(
		#EAAFC8, 
		#ffffff);
	box-shadow: -3px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
}
.blue {
  background-image: radial-gradient(
		#6DD5ED, 
		#ffffff);
	box-shadow: -3px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
}
.violet {
  background-image: radial-gradient(
		#654EA3, 
		#ffffff);
	box-shadow: -3px 3px 6px 0px 
		rgba(0, 0, 0, 0.16);
}
.dark {
  background-image: radial-gradient(
		#172C5C, 
		#ffffff);
	box-shadow: 0px 3px 6px 0px 
		rgba(0, 0, 0, 0.16);
}
.green {
  background-image: radial-gradient(
		#99F2C8, 
		#ffffff);
	box-shadow: -3px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
}
.orange {
  background-image: radial-gradient(
		#F7971E, 
		#ffffff);
	box-shadow: -3px 3px 10px 0px 
		rgba(0, 0, 0, 0.16);
}